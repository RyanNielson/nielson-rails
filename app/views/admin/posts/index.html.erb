<div class="page-header">
  <h1>Posts <%= link_to 'New', new_admin_post_path, class: 'btn btn-success' %></h1>
</div>

<div class="table-responsive">
  <table class="table">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Title</th>
          <th>Slug</th>
          <th>Published at</th>
          <th colspan="3">Actions</th>
        </tr>
      </thead>
      <tbody>
        <%= render @posts %>
     </tbody>
    </table>
  </table>
</div>
